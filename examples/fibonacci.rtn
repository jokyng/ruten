def fibonacci(n):
    if n <= 1:
        return n
    
    a = 0
    b = 1
    
    for i in range(2, n + 1):
        temp = a + b
        a = b
        b = temp
    
    return b

print("fibonacci calculator")
print("--------------------")
print()

for i in range(0, 15):
    result = fibonacci(i)
    print("fib(" + str(i) + ") =", result)
